module ToLatex (ToLatex (..), toLatex)
  where

class ToLatex a where
  toLatex :: a -> String

instance (ToLatex a, ToLatex b) => ToLatex (a, b)
  toLatex (a, b) = "(" ++ toLatex a ++ ", " ++ toLatex b ++ ")"


instance (ToLatex a, ToLatex b, ToLatex c) => ToLatex (a, b, c)
  toLatex (a, b, c) = "(" ++ toLatex a ++ ", " ++ toLatex b ++ ", " ++ toLatex c ++ ")"
